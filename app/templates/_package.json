{
    "name": "<%= basename %>",
    "version": "0.0.0",
    "main": "app.js",
    "scripts": {
        "start": "NODE_ENV=local DEBUG=<%= basename%> node --harmony app.js",
        "test": "mocha --harmony"
    },
    "dependencies": {
        "koa": "~1.2.0",
        "koa-router": "~5.4.0",
        "koa-compress": "~1.0.9",
        "koa-static-cache": "~3.1.7",
        <% if(db==="none"){ %>
        "koa-session": "~3.3.1",
        <% }else{ %>
        "koa-generic-session": "~1.10.2",
        <% }%>
        <% if(db==="redis"){ %>
        "koa-redis": "~2.1.1",
        <% }%>
        <% if(db==="mongodb"){ %>
        "koa-mongo": "~0.5.0",
        "koa-generic-session-mongo": "~0.2.5",
        <% }%>
        "koa-validator": "~0.6.4",
        "koa-bodyparser": "~2.2.0",
        "koa-onerror": "~1.3.1",
        "lru-cache": "~4.0.1",
        "mini-logger": "~1.1.0",
        "xtemplate": "~4.5.1",
        "xtpl": "~3.2.0",
        "debug": "~2.0.0",
        "co-urllib": "~0.2.3",
        "co": "~4.6.0",
        "underscore": "~1.8.3"
    },
    "devDependencies": {
        "supertest": "^0.12.1",
        "mocha": "^1.18.2",
        "should": "3.3.2"
    }
}